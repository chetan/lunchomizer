<html>
<head>
	<title>the midtown lunch-o-mizer!</title>
	
	<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js" type="text/javascript" charset="utf-8"></script>
	
	<%= stylesheet_link_tag "960", :media => "screen, projection" %>
	<%= stylesheet_link_tag "reset", :media => "screen, projection" %>
	<%= stylesheet_link_tag "text", :media => "screen, projection" %>
	<%= stylesheet_link_tag "lunch" %>
	
	<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
	
	<script type="text/javascript">
    	function addCodeToFunction(func, code) {
            if (func == undefined)
        	    return code;
            else {
        	    return function(){
        	        func();
        	        code();
        	    }
            }
        }
    </script>
	
	<%= @close_map.header_html %>
	<%= @near_map.header_html %>
	<%= @far_map.header_html %>

</head>
<body>

<div class="container_12">

<div class="grid_2">&nbsp;</div>

<div class="grid_8">
	
	<h1>the midtown lunch-o-mizer!</h1>
	
<%= yield %>
</div>

<div class="grid_2">&nbsp;</div>

</body>
</html>